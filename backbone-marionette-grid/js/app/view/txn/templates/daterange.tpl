<div class="arrow-back"></div>
<div class="arrow-font"></div>
<div class="fast-btns-wrap">
    <ul class="inner no-wrap">
        <li><button class="btn-today">今天</button></li>
        <li><button class="btn-this-week">本周</button></li>
        <li><button class="btn-this-month">本月</button></li>
        <a class="clearfix"></a>
    </ul>
</div>
<div class="date-range">
    <div class="inner">
        <div class="start select-line">
            从<select class="start-y y"></select>年
            <select class="start-m m"></select>月
            <select class="start-d d"></select>日
        </div>
        <div class="end select-line">
            至<select class="end-y y"></select>年
            <select class="end-m m"></select>月
            <select class="end-d d"></select>日
        </div>
    </div>
</div>
<div class="help-error-sit unit-inner">
    <label hidden class="error"></label>
</div>
<div class="bottom">
    <div class="ok btn pull-left">确定</div>
    <div class="cancel btn pull-left">取消</div>
    <a class="clearfix"></a>
</div>